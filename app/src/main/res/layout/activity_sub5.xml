<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.lmj.firstapp.SubActivity5">



    <Button
        android:id="@+id/button24"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="돌아가기"
        android:textColor="#ff000000"
        android:textSize="20dp"
        android:onClick="onButton_Finish"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <Button
        android:id="@+id/buttonT0"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Button\nButton"
        android:textSize="20dp"
        android:onClick="onButton_TimeSec0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonT1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Button\nButton"
        android:textSize="20dp"
        android:onClick="onButton_TimeSec1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonT0" />

    <Button
        android:id="@+id/buttonT2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Button\nButton"
        android:textSize="20dp"
        android:onClick="onButton_TimeSec2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonT1" />
</android.support.constraint.ConstraintLayout>
